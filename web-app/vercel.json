{
  "framework": "nextjs",
  "regions": ["sin1"],
  "functions": {
    "app/api/*/route.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Powered-By",
          "value": "IPL Dashboard"
        }
      ]
    }
  ]
}