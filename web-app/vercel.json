{
  "framework": "nextjs",
  "regions": ["sin1"],
  "functions": {
    "app/api/*/route.ts": {
      "maxDuration": 60,
      "memory": 2048
    }
  },
  "env": {
    "PUPPETEER_CACHE_DIR": "/tmp/.cache/puppeteer",
    "FONTCONFIG_PATH": "/tmp/.fonts"
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Powered-By",
          "value": "IPL Dashboard"
        }
      ]
    }
  ]
}